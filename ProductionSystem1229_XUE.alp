			<Functions>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1761997421325</Id>
					<Name><![CDATA[procMinutes]]></Name>
					<Description><![CDATA[纯加工时间]]></Description>
					<X>40</X><Y>260</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[o]]></Name>
						<Type><![CDATA[order]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[machineId]]></Name>
						<Type><![CDATA[String]]></Type>
					</Parameter>
					<Body><![CDATA[double qty = o.p_order_qty_units;

// scrap 兜底
Double sObj = scrap.get(o.p_sku);
double s = (sObj == null) ? 0.0 : sObj.doubleValue();
double qtyNet = qty / Math.max(1e-9, (1.0 - s));

// 选择速率（件/小时）
Double rObj = null;
if ("New_X".equals(machineId)) {
  rObj = rateNew.get(o.p_sku);
} else if ("Old_A".equals(machineId)) {
  rObj = rateOldA.get(o.p_sku);
} else if ("Old_B".equals(machineId)) {
  rObj = rateOldB.get(o.p_sku);
}
double r = (rObj == null) ? 0.0 : rObj.doubleValue();

if (r <= 0) {
  warning("No rate for SKU " + o.p_sku + " on " + machineId + ". Fallback 1 u/h.");
  r = 1.0;
}

return qtyNet / r * 60.0;
]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[String]]></ReturnType>
					<Id>1762007910912</Id>
					<Name><![CDATA[loadSkuMaster]]></Name>
					<Description><![CDATA[把 SKU master 表 → Java 里的 Map 数据结构，为后面 procMinutes 和换线逻辑提供基础数据。]]></Description>
					<X>40</X><Y>290</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[excelPath]]></Name>
						<Type><![CDATA[String]]></Type>
					</Parameter>
					<Body><![CDATA[rateNew.clear();
rateOldA.clear();
rateOldB.clear();
scrap.clear();
skuFamily.clear();

try (FileInputStream fis = new FileInputStream(excelPath);
     Workbook wb = WorkbookFactory.create(fis)) {

  Sheet s = wb.getSheetAt(0);           // 第一张工作表
  Iterator<Row> it = s.iterator();
  if (it.hasNext()) it.next();          // 跳过表头

  while (it.hasNext()) {
    Row r = it.next();
    if (r.getCell(0) == null) continue;

    String sku = r.getCell(0).getStringCellValue().trim();  // A列 sku
    if (sku.isEmpty()) continue;

    String fam = r.getCell(1).getStringCellValue().trim();  // B列 family
    double rNew = r.getCell(4).getNumericCellValue();       // E列 proc_rate_new_u_per_h
    double rA   = r.getCell(5).getNumericCellValue();       // F列 proc_rate_oldA_u_per_h
    double rB   = r.getCell(6).getNumericCellValue();       // G列 proc_rate_oldB_u_per_h
    double sRate= r.getCell(7).getNumericCellValue();       // H列 scrap_rate

    rateNew.put(sku, rNew);
    rateOldA.put(sku, rA);
    rateOldB.put(sku, rB);
    scrap.put(sku, sRate);
    skuFamily.put(sku, fam);
  }

  trace("Loaded SKU master: " + rateNew.size());

} catch (Exception e) {
  error("loadSkuMaster: " + e.getMessage());
}]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
