public java.util.List<String[]> orderLog = new java.util.ArrayList<>();
// 把仿真时间（单位 = 分钟）转换成真实日历时间，从 2024-01-01 00:00:00 开始
public java.util.Date simTimeToDate(double simTimeMinutes) {
    // 起始日历时间（你的模型起始日期，可以改）
    java.util.Calendar cal = java.util.Calendar.getInstance();
    cal.set(2024, java.util.Calendar.JANUARY, 1, 0, 0, 0);  // 2024-01-01 00:00:00
    cal.set(java.util.Calendar.MILLISECOND, 0);

    // 模拟时间转毫秒（当前 time() 如果是“小时”为时间单位）
    long deltaMillis = (long) (simTimeMinutes * 60_000.0);
    
    cal.setTimeInMillis(cal.getTimeInMillis() + deltaMillis);
    return cal.getTime();
}

// 格式化日期：Date → yyyy-MM-dd HH:mm:ss
public String formatDate(java.util.Date d) {
    java.text.SimpleDateFormat f =
        new java.text.SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
    return f.format(d);
}

// 把真实日期 (crea_ion_s / due_s) 转成模拟时间（单位：分钟）
public double dateToSimMinutes( java.util.Date realDate ) {

    java.util.Calendar cal0 = java.util.Calendar.getInstance();
    cal0.set(2024, java.util.Calendar.JANUARY, 1, 0, 0, 0);
    cal0.set(java.util.Calendar.MILLISECOND, 0);

    long baseMillis = cal0.getTimeInMillis();
    long diffMillis = realDate.getTime() - baseMillis;

    return diffMillis / 60_000.0;   // 毫秒 → 分钟
}

