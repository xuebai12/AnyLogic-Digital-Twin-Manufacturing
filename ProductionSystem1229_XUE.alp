							<Value Class="CodeValue">
								<Code><![CDATA[order o = (order) agent;

// 从数据库列赋值
o.qtyOrdered = o.p_order_qty_units;

// 记录进入系统时间
o.tCreated = time();

// 把 p_due_ts (java.util.Date) 转成模拟时间（分钟）
o.dueTimeSim = dateToSimMinutes( o.p_due_ts );

// 初始化实际产出数量
o.qtyProduced = 0;

// KPI：总订单数 +1
kpi_totalOrders++;

// KPI：WIP++
kpi_WIP++;
double now = time();
kpi_WIP_area += kpi_WIP * (now - kpi_WIP_lastTime);
kpi_WIP_lastTime = now;
]]></Code>
