				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[void]]></ReturnType>
					<Id>1765056284046</Id>
					<Name><![CDATA[exportScenarioKpiBasic]]></Name>
					<X>40</X><Y>500</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[
Main m = (Main) getEngine().getRoot();

    // ====================================================
    // 1) FINAL KPI 计算（必须写回 Main 变量）
    // ====================================================

    // --- Flow time ---
    m.kpi_avgFlowTime = (m.kpi_completedOrders > 0)
    ? (m.kpi_sumFlowTime / m.kpi_completedOrders)
    : 0.0;

     m.kpi_avgWIP = (time() > 0)
    ? (m.kpi_WIP_area / time())
    : 0.0;

    m.kpi_OTD = (m.kpi_totalOrders > 0)
    ? (1.0 * m.kpi_OTD_count / m.kpi_totalOrders)
    : 0.0;

    m.kpi_OTIF = (m.kpi_totalOrders > 0)
    ? (1.0 * m.kpi_OTIF_count / m.kpi_totalOrders)
    : 0.0;

    // ====================================================
    // 2) OEE 计算
    // ====================================================

    // ---------- Quality Q ----------
   m.qualA = (m.oee_totalUnits_OldA > 0)
      ? (1.0 * m.oee_goodUnits_OldA / m.oee_totalUnits_OldA)
      : 0.0;

    m.qualB = (m.oee_totalUnits_OldB > 0)
       ? (1.0 * m.oee_goodUnits_OldB / m.oee_totalUnits_OldB)
       : 0.0;

    m.qualX = (m.oee_totalUnits_NewX > 0)
       ? (1.0 * m.oee_goodUnits_NewX / m.oee_totalUnits_NewX)
       : 0.0;
// ---------- Availability A ----------
// 2024：262 day * 16 hour * 60 min
double plannedTimeMin = 262 * 16 * 60;

// Old_A
if (m.oee_actualProcMin_OldA > 0) {
    m.availA = Math.max(0,
        (plannedTimeMin - (m.oee_breakdownMin_OldA + m.oee_pmMin_OldA)) / plannedTimeMin);
} else {
    m.availA = 0;
}

// Old_B
if (m.oee_actualProcMin_OldB > 0) {
    m.availB = Math.max(0,
        (plannedTimeMin
         - (m.oee_breakdownMin_OldB + m.oee_pmMin_OldB))
        / plannedTimeMin);
} else {
    m.availB = 0;
}

// New_X
if (m.oee_actualProcMin_NewX > 0) {
    m.availX = Math.max(0,
        (plannedTimeMin
         - (m.oee_breakdownMin_NewX + m.oee_pmMin_NewX))
        / plannedTimeMin);
} else {
    m.availX = 0;
}

    // ---------- Performance P ----------
   
m.perfA = (m.oee_actualProcMin_OldA > 0)
    ? (m.oee_idealProcMin_OldA / m.oee_actualProcMin_OldA)
    : 0.0;

m.perfB = (m.oee_actualProcMin_OldB > 0)
    ? (m.oee_idealProcMin_OldB / m.oee_actualProcMin_OldB)
    : 0.0;

m.perfX = (m.oee_actualProcMin_NewX > 0)
    ? (m.oee_idealProcMin_NewX / m.oee_actualProcMin_NewX)
    : 0.0;
    

    // ---------- OEE = A × P × Q ----------
    m.kpi_oeeA = availA * perfA * qualA;
    m.kpi_oeeB = availB * perfB * qualB;
    m.kpi_oeeX = availX * perfX * qualX;
  
    

    // ====================================================
    // 3) CSV 导出
    // ====================================================

    java.io.File folder = new java.io.File(logFolder);
    folder.mkdirs();

    java.io.File file = new java.io.File(folder, "scenario_kpi_basic.csv");
    boolean newFile = !file.exists();

    try (java.io.FileWriter fw = new java.io.FileWriter(file, true);
         java.io.PrintWriter out = new java.io.PrintWriter(fw)) {

        if (newFile) {
            out.println(
                "machineConfig," +
                "avgFlowTime_min,avgWIP," +
                "OTD,OTIF," +
                "logisticsMode," +
                "totalLogisticsCost_EUR,avgLogisticsCostPerOrder_EUR," +
                "A_OldA,P_OldA,Q_OldA,OEE_OldA," +
                "A_OldB,P_OldB,Q_OldB,OEE_OldB," +
                "A_NewX,P_NewX,Q_NewX,OEE_NewX"
            );
        }

        out.println(
            machineConfig + "," +
            kpi_avgFlowTime + "," +
            kpi_avgWIP + "," +
            kpi_OTD + "," +
            kpi_OTIF + "," +
            logisticsMode + "," +
            kpiTotalLogisticsCostEur + "," +
            kpiAvgLogisticsCostPerOrder + "," +

            availA + "," + perfA + "," + qualA + "," + kpi_oeeA + "," +
            availB + "," + perfB + "," + qualB + "," + kpi_oeeB + "," +
            availX + "," + perfX + "," + qualX + "," + kpi_oeeX
        );

    } catch (Exception e) {
        error("exportScenarioKpiBasic(): " + e.getMessage());
    }

]]></Body>
