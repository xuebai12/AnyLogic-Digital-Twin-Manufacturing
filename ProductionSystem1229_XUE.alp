					<Name><![CDATA[startMaintenance]]></Name>
					<X>40</X><Y>320</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[task]]></Name>
						<Type><![CDATA[pmTasks]]></Type>
					</Parameter>
					<Body><![CDATA[traceln("PM START  machine=" + task.machineId 
            + "  duration_h=" + task.pmDurationH);

    double pmMin = task.pmDurationH * 60.0;

    if ("Old_A".equals(task.machineId)) {
        oee_pmMin_OldA += pmMin;
        machine_Old_A.capacity = 0;
    } else if ("Old_B".equals(task.machineId)) {
        oee_pmMin_OldB += pmMin;
        machine_Old_B.capacity = 0;
    } else if ("New_X".equals(task.machineId)) {
        oee_pmMin_NewX += pmMin;
        machine_New_X.capacity = 0;
    }]]></Body>
