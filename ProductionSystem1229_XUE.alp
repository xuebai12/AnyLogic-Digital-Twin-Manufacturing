						<Parameter>
							<Name><![CDATA[onExit]]></Name>
							<Value Class="CodeValue">
								<Code><![CDATA[order o = (order) agent;
String mid = o.lastMachineId; 


// 目标良品数 (即订单要求的数量，例如 100)
double targetGood = o.p_order_qty_units;

// 获取废品率 
Double sObj = scrap.get(o.p_sku);
double scrapRate = (sObj == null) ? 0.0 : sObj.doubleValue();


// 公式：为了得到 100 个良品，必须投入 100 / (1 - 0.05) = 106 个
double qtyTotalReal = Math.ceil(targetGood / Math.max(1e-9, (1.0 - scrapRate)));
// 所以良品数应该等于订单要求数
double qtyGoodReal = targetGood;

// 将实际良品数存入 agent (用于后续 Sink 判断 OTIF)
o.qtyProduced = (int) qtyGoodReal; 

// ====================================================
// 2.  (Performance)
// ====================================================
// 获取该 SKU 在该机器上的标准速率 (Units per Hour)
double idealRate = 1.0; 
if ("New_X".equals(mid)) {
    idealRate = rateNew.containsKey(o.p_sku) ? rateNew.get(o.p_sku) : 1.0;
} else if ("Old_A".equals(mid)) {
    idealRate = rateOldA.containsKey(o.p_sku) ? rateOldA.get(o.p_sku) : 1.0;
} else if ("Old_B".equals(mid)) {
    idealRate = rateOldB.containsKey(o.p_sku) ? rateOldB.get(o.p_sku) : 1.0;
}
if (idealRate <= 0) idealRate = 1.0;

// [核心修复] 计算 Ideal Time 
double idealMin = (qtyTotalReal / idealRate) * 60.0;

// 获取 Actual Time 
double actualMin = o.procMinutes;

// ====================================================
// 3. 更新全局 KPI 统计变量
// ====================================================
if ("Old_A".equals(mid)) {
    oee_totalUnits_OldA    += qtyTotalReal;  
    oee_goodUnits_OldA     += qtyGoodReal;   
   
    oee_idealProcMin_OldA  += idealMin;    
    oee_actualProcMin_OldA += actualMin;     // P = Ideal/Actual ≈ 100% (正确)
    
} else if ("Old_B".equals(mid)) {
    oee_totalUnits_OldB    += qtyTotalReal;
    oee_goodUnits_OldB     += qtyGoodReal;
    
    oee_idealProcMin_OldB  += idealMin;    
    oee_actualProcMin_OldB += actualMin;   
    
} else if ("New_X".equals(mid)) {
    oee_totalUnits_NewX    += qtyTotalReal;
    oee_goodUnits_NewX     += qtyGoodReal;
    
    oee_idealProcMin_NewX  += idealMin;    
    oee_actualProcMin_NewX += actualMin;   
}


traceln("ORDER " + o.p_order_id 
    + " | Machine=" + o.lastMachineId
    + " | SKU=" + o.p_sku 
    + " | Input=" + (int)qtyTotalReal 
    + " | Output=" + (int)qtyGoodReal
    + " | ActualMin=" + actualMin);]]></Code>
