							<Name><![CDATA[onEnter]]></Name>
							<Value Class="CodeValue">
								<Code><![CDATA[Main m = (Main) getEngine().getRoot();
// 只声明一次，并带上类型转换
order o = (order)agent;

// 完成时间
o.tFinished = time();

// Flow time
double flow = o.tFinished - o.tCreated;
kpi_sumFlowTime += flow;
kpi_completedOrders++;    
if (flow > kpi_maxFlowTime) kpi_maxFlowTime = flow;

// WIP --
double now = time();
kpi_WIP_area += kpi_WIP * (now - kpi_WIP_lastTime);
kpi_WIP_lastTime = now;
kpi_WIP--;

// =====Logistics cost  =====

// 1) 当前物流方式的每小时成本 (€ / h)
double costPerHour = getLogisticsCostPerHour();

// 2) 该订单物流时间（分钟） → 小时
double timeHours = o.logisticsTimeMinTotal / 60.0;

// 3) 该订单物流成本
o.logisticsCostEur = timeHours * costPerHour;

// 4) 更新全局 KPI（先累加成本）
kpiTotalLogisticsCostEur += o.logisticsCostEur;

// 5) 再计算平均每单成本
kpiAvgLogisticsCostPerOrder =
    kpiTotalLogisticsCostEur / kpi_completedOrders;

// ===== OTD / OTIF 部分 =====

// 交期（OTD / OTIF）
boolean isOTD = o.tFinished <= o.dueTimeSim;

o.qtyProduced = o.qtyOrdered; 

boolean isOTIF = isOTD && (o.qtyProduced >= o.qtyOrdered);

if (isOTD)  kpi_OTD_count++;
if (isOTIF) kpi_OTIF_count++;

// ===== 调试输出 =====
traceln("SINK | Order=" + o.p_order_id
    + " | Flow=" + flow
    + " | OTD=" + isOTD
    + " | OTIF=" + isOTIF
    + " | CompletedOrders=" + kpi_completedOrders);
        
orderLog.add(new String[]{
    o.p_order_id,
    o.p_sku,
    o.p_priority,
    formatDate(simTimeToDate(o.tCreated)),
    formatDate(simTimeToDate(o.tFinished)),
    Double.toString(o.tFinished - o.tCreated),
    Double.toString(o.dueTimeSim),
    Boolean.toString(o.tFinished <= o.dueTimeSim),                  // isOTD
    Boolean.toString(o.tFinished <= o.dueTimeSim 
                     && o.qtyProduced >= o.qtyOrdered),             // isOTIF
    Integer.toString(o.qtyOrdered),
    Integer.toString(o.qtyProduced),
    Double.toString(o.logisticsTimeMinTotal),
    Double.toString(o.logisticsCostEur),
    o.lastMachineId    
});
]]></Code>
							</Value>
